{
  "*.c": {
    "*.c": {
      "alternate": "./{}.h"
    },
    "*.h": {
      "alternate": "./{}.c"
    }
  },
  "locals/playgrounds/": {
    "locals/playgrounds/*.rb": {
      "type": "playground",
      "timeline": true
    }
  },
  "Podfile": {
    "*.m": {
      "alternate": "./{}.h"
    },
    "*.h": {
      "alternate": "./{}.m"
    }
  },
  "*.cpp": {
    "*.cpp": {
      "alternate": "./{}.h"
    },
    "*.h": {
      "alternate": "./{}.cpp"
    }
  }
}
